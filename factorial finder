#include <stdio.h>
//factorial 
void factorial(int n){
    int i=1,fact=1;
    for (i;i<=n;i++){
        fact*=i;
    }
    printf("Factorial of the number is %d",fact);
}
int main(){
    int n;
    printf("Enter the number whose factorial is required : ");
    scanf("%d",&n);
    factorial(n);
}
