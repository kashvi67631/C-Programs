//nums1 = [1,2,2,1]
//nums2 = [2,2] takes out common
#include <stdio.h>
int main(){
    int s1=4,s2=2; //size of n1=s1,size of n2=s2
    int n1[4]={1,2,2,1},n2[2]={2,2},l=0,equal[l];
    //selection sort
    if (s2<s1){
    for (int i=0;i<s2;i++){
        for (int j=0;j<s1;j++){
            if (n2[i]==n1[j] && !equal[j]){
                equal[l]=n1[j];
                l++;
            }
        }
    }
    }
    for (int k=0;k<l;k++){
        printf("%d ",equal[k]);
    }
    return 0;
}
