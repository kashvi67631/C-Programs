#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <string.h>
void rolldice(){
    int dice=(rand()%10)+1; 
    printf("%d\n",dice);   
}
int main(){
    printf("===Welcome to Roll The Dice===\n");
    srand(time(0));
    while (1){    
        char ch;
        printf("enter n / y to roll the dice\n");
        scanf("%c/n",&ch);
    if (ch=='n'){
        break;
    }
    else{
        rolldice();
    }}
    return 0;
}
