#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <string.h>
//number guessing random number
int randm(){
    srand(time(0));
    int num=((rand()%10)+1);
    return num;
}
int main(){
    printf("Welcome to guess a number game\n");
    printf("Processing...\n");
    printf("okay number selected\n");
    int x;
    x=randm();
    while (1){
    int ch;
    printf("guess one number from 1-10\n");
    scanf(" %d",&ch);
    if (ch==x){
        printf("Yess the number is %d\n",x);
    }
    else{
        printf("The gussed number is not right .\n");
    }
    char ask;
    printf("Do you want to play again?-y/n\n");
    scanf(" %c",&ask);
    if (ask=='n'){
        break;
    }
    }
    return 0;
}
